{
  "name": "dcarbone/gotime",
  "type": "library",
  "license": "MIT",
  "description": "Golang-like time class(es) for PHP 7.2+",
  "homepage": "https://github.com/dcarbone/gotime",
  "authors": [
    {
      "name": "The GOLANG Squad",
      "homepage": "https://golang.org/"
    },
    {
      "name": "Daniel Carbone",
      "email": "daniel.p.carbone@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*"
  },
  "autoload": {
    "files": [
      "src/precision.php"
    ],
    "psr-4": {
      "DCarbone\\Go\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload-dev": {
    "files": [
      "src/precision.php"
    ],
    "psr-4": {
      "DCarbone\\Go\\": "src/",
      "DCarbone\\GoTimeTests\\": "tests/"
    }
  }
}